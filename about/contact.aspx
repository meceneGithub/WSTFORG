                            <article class="grid_16">
                                <h3>Contact form</h3>
                                <form id="form1" runat="server">
                                    <div class="success"> Contact form submitted!<br />
                                    
                                    <strong>We will be in touch soon.</strong> </div>
                                    <fieldset  class="formStyle">
                                                <label class="name rel">
                                                    Name:
                                                    <span class="inp">
                                                        <input type="text" runat="server" id="userName" style="font-size:16px; color:#333" />
                                                            <asp:RequiredFieldValidator ID="RequiredFieldValidator1" runat="server" ErrorMessage="Please enter your full name" ControlToValidate="userName"></asp:RequiredFieldValidator>
                                                    </span>
                                                    <span class="error">*Not a valid name.</span> <span class="empty">*This field is required.</span>
                                                </label>
                                        <br />
                                                <label class="email rel">
                                                    Email:
                                                    <span class="inp">
                                                    <input type="text" runat="server" id="userEmail" style="font-size:16px; color:#333" />
                                                    <asp:RequiredFieldValidator ID="RequiredFieldValidator2" runat="server" ErrorMessage="Please enter your email address" ControlToValidate="userEmail"></asp:RequiredFieldValidator>                                                       
                                                    <asp:RegularExpressionValidator ID="RegularExpressionValidator1" runat="server" ErrorMessage="Please enter a valid email address" ControlToValidate="userEmail" ValidationExpression="\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*"></asp:RegularExpressionValidator>                  
                                                    </span>
                                                    <span class="error">*Not a valid email.</span> <span class="empty">*This field is required.</span>
                                                </label>
                                        <br />
                                                <label class="phone rel">
                                                    Phone:
                                                    <span class="inp">
                                                        <input type="text" runat="server" id="userPhone" name="txtInput"  onkeydown="javascript:backspacerDOWN(this,event);" onkeyup="javascript:backspacerUP(this,event);" style="font-size:16px; color:#333"/>
                                                        <asp:RequiredFieldValidator ID="RequiredFieldValidator3" runat="server" ErrorMessage="Please enter your phone number" ControlToValidate="userPhone"></asp:RequiredFieldValidator>
                                                        <asp:RegularExpressionValidator ID="RegularExpressionValidator2" runat="server" ErrorMessage="Please enter a valid phone number" ControlToValidate="userPhone" ValidationExpression="((\(\d{3}\) ?)|(\d{3}-))?\d{3}-\d{4}"></asp:RegularExpressionValidator>                                                    
                                                    </span>
                                                    <span class="error">*Not a valid phone number.</span> <span class="empty">*This field is required.</span>
                                                </label>
                                        <br />
                                                <label class="message rel">
                                                    Message:
                                                    <span class="text_a">
                                                        <textarea runat="server" id="userMessage" style="font-size:16px; color:#333" rows="10" cols="10"></textarea>
                                                        <asp:RequiredFieldValidator ID="RequiredFieldValidator4" runat="server" ErrorMessage="Please enter a short message" ControlToValidate="userMessage"></asp:RequiredFieldValidator>
                                                    </span>
                                                    <span class="error">*The message is too short.</span> <span class="empty">*This field is required.</span>
                                                </label>
                                        <div class="buttons-wrapper">
                                             <asp:Button ID="Button1" runat="server" OnClick="Button1_Click" Text="Submit" Width="150px" Height="40px" BackColor="#3366FF" BorderColor="White" 
                                                         BorderStyle="Solid" Font-Bold="True" Font-Size="Medium" ForeColor="White"  />
                                        </div>
                                        <asp:ValidationSummary ID="ValidationSummary1" runat="server" Font-Italic="False" DisplayMode="BulletList" ShowMessageBox="True" ShowSummary="False"></asp:ValidationSummary>
                                    </fieldset>
                                </form>

