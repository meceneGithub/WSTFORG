                            <article class="grid_16">
                                <h3>Registration Form</h3>
                                <form id="form1" runat="server">
                                    <div class="success"> Contact form submitted!<br />
                                    
                                    <strong>We will be in touch soon.</strong> </div>
                                    <fieldset  class="formStyle">
                                                <label class="name rel">
                                                    Name:
                                                    <span class="inp">
                                                        <input type="text" runat="server" id="userName" style="font-size:16px; color:#333">
                                                        <asp:RequiredFieldValidator ID="RequiredFieldValidator1" runat="server" ErrorMessage="Please enter your full name" ControlToValidate="userName" Display="Dynamic" ForeColor="#FFFF99" SetFocusOnError="True">enter your full name</asp:RequiredFieldValidator>
                                                    </span>
                                                    <span class="error">*Not a valid name.</span> <span class="empty">*This field is required.</span>
                                                </label>
                                        <br />
                                         <label class="name rel">
                                                    Church Name:
                                                    <span class="inp">
                                                        <input type="text" runat="server" id="userChurch" style="font-size:16px; color:#333">
                                                        <asp:RequiredFieldValidator ID="RequiredFieldValidator2" runat="server" ErrorMessage="Please enter your churh name" ControlToValidate="userChurch">enter your church</asp:RequiredFieldValidator>

                                                    </span>
                                                    <span class="error">*Not a valid name.</span> <span class="empty">*This field is required.</span>
                                                </label>
                                        <br />
                                                <label class="email rel">
                                                    Email:
                                                    <span class="inp">
                                                        <input type="text" runat="server" id="userEmail" style="font-size:16px; color:#333">
                                                        <asp:RegularExpressionValidator ID="RegularExpressionValidator1" runat="server" ErrorMessage="Please enter a valid email address" ControlToValidate="userEmail" ValidationExpression="\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*">Invalid email. please enter a valid email address</asp:RegularExpressionValidator>
                                                        <asp:RequiredFieldValidator ID="RequiredFieldValidator3" runat="server" ErrorMessage="Please enter your email address" ControlToValidate="userEmail">enter your email address</asp:RequiredFieldValidator>
                                                    </span>
                                                    <span class="error">*Not a valid email.</span> <span class="empty">*This field is required.</span>
                                                </label>
                                        <br />
                                                <label class="phone rel">
                                                    Phone:
                                                    <span class="inp">
                                                        <input type="text" runat="server" id="userPhone" name="txtInput"  onkeydown="javascript:backspacerDOWN(this,event);" onkeyup="javascript:backspacerUP(this,event);" style="font-size:16px; color:#333"/>
                                                        <asp:RegularExpressionValidator ID="RegularExpressionValidator2" runat="server" ErrorMessage="Please enter a valid phone number" ControlToValidate="userPhone" ValidationExpression="((\(\d{3}\) ?)|(\d{3}-))?\d{3}-\d{4}"></asp:RegularExpressionValidator>
                                                        <asp:RequiredFieldValidator ID="RequiredFieldValidator4" runat="server" ErrorMessage="Please enter your phone number" ControlToValidate="userPhone">Please enter phone number.</asp:RequiredFieldValidator>
                                                    </span>
                                                    <span class="error">*Not a valid phone number.</span> <span class="empty">*This field is required.</span>
                                                </label>
                                        <br />
                                                 <label class="State rel">
                                                     State:
                                                    <span class="inp">
                                                        <input type="text" runat="server" id="userState" style="font-size:16px; color:#333">                                                        
                                                        <asp:RequiredFieldValidator ID="RequiredFieldValidator5" runat="server" ErrorMessage="Please enter your state" ControlToValidate="userState">Please enter your state</asp:RequiredFieldValidator>
                                                    </span>
                                                    <span class="error">*Not a valid State.</span> <span class="empty">*This field is required.</span>
                                                </label>
                                        <br />
                                         <label class="State rel">
                                                Please select your room preference:
                                            <br />
                                                <asp:DropDownList ID="DropDownList1" runat="server" Width="350px" Height="24px" BackColor="#FFFF99">
                                                    <asp:ListItem Value="$170" Text="4 Persons per room" Enabled="true">4 Persons per room - $170/person</asp:ListItem>
                                                    <asp:ListItem Value="$185" Text="3 Persons per room">3 Persons per room - $185/person</asp:ListItem>
                                                    <asp:ListItem Value="$200" Text="2 Persons per room">2 Persons per room - $200/person</asp:ListItem>
                                                    <asp:ListItem Value="$260" Text="Single room">Single Room - $260/person</asp:ListItem>
                                                </asp:DropDownList>                                                 
                                            </label>
                                        <asp:ValidationSummary ID="ValidationSummary1" runat="server" ShowMessageBox="True" ShowSummary="False"></asp:ValidationSummary>
                                        <div class="buttons-wrapper">
                                             <asp:Button ID="Button1" runat="server" OnClick="Button1_Click" Text="Submit" 
                                          Width="150px" Height="40px" BackColor="#3366FF" BorderColor="White" 
                                          BorderStyle="Solid" Font-Bold="True" Font-Size="Medium" ForeColor="White"  />
                                        </div>
                                    </fieldset>
